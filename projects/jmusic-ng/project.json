{
  "name": "jmusic-ng",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "projects/jmusic-ng/src",
  "prefix": "mus",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:ng-packagr",
      "options": {
        "project": "projects/jmusic-ng/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "projects/jmusic-ng/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "projects/jmusic-ng/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production",
      "outputs": ["{workspaceRoot}/dist/jmusic-ng"]
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "projects/jmusic-ng/src/test.ts",
        "tsConfig": "projects/jmusic-ng/tsconfig.spec.json",
        "karmaConfig": "projects/jmusic-ng/karma.conf.js"
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "projects/jmusic-ng/.storybook",
        "browserTarget": "jmusic-ng:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/jmusic-ng",
        "configDir": "projects/jmusic-ng/.storybook",
        "browserTarget": "jmusic-ng:build-storybook",
        "compodoc": false
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "test-storybook": {
      "executor": "nx:run-commands",
      "options": {
        "command": "test-storybook -c projects/jmusic-ng/.storybook --url=http://localhost:4400"
      }
    }
  }
}
